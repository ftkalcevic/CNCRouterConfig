O<probe-center> sub
G21 F25

#1000=#1 ;max radius


G90 G30.1  ;Save current coordinate
G91 G38.3 Y#1000  ;Scan +Y limit
#100 = #5062 ;Save +Y limit
G90 G30  ;Back to start cordinate
G91 G38.3 Y[-1*#1000] ;Scan -Y limit
G90 G0Y[[#5062+#100]/2]  ;goto aprox Y center

G90 G30.1  ;Save current coordinate
G91 G38.3 X#1000  ;Scan +X limit
#101 = #5061 ;Save +X limit
G90 G30  ;Back to start cordinate
G91 G38.3 X[-1*#1000] ;Scan -X limit
G90 G0X[[#5061+#101]/2]  ;goto exact X center
#101= [#101-#5061]  ;calculate span

G90 G30.1  ;Save current coordinate
G91 G38.3 Y#1000  ;Scan +Y limit
#100 = #5062 ;Save +Y limit
G90 G30  ;Back to start cordinate
G91 G38.3 Y[-1*#1000] ;Scan -Y limit
G90 G0Y[[#5062+#100]/2]  ;goto exact Y center
#100= [#100-#5062]  ;calculate span
#102=[#100-#101]    ;excentricy


(print, X span= #101)
(print, Y span= #100)
(print, excentricy #102)


O<probe-center> endsub
